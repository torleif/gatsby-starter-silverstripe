    "start": "ENABLE_GATSBY_REFRESH_ENDPOINT=1 npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unclecheese/gatsby-starter-silverstripe"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
