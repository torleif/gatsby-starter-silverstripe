{
  "name": "gatsby-starter-silverstripe",
  "private": true,
  "description": "A starter for a Gatsby project that uses Silverstripe CMS as a data source",
  "version": "0.1.0",
  "author": "Uncle Cheese <unclecheese@leftandmain.com>",
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2",
    "gatsby-image": "^2.2",
    "gatsby-plugin-manifest": "^2.2",
    "gatsby-plugin-offline": "^3.0",
    "gatsby-plugin-react-helmet": "^3.1",
    "gatsby-plugin-sass": "^2.1",
    "gatsby-plugin-sharp": "^2.2",
    "gatsby-plugin-tslint": "^0.0.2",
    "gatsby-plugin-typescript": "^2.1",
    "gatsby-source-filesystem": "^2.1",
    "gatsby-source-silverstripe": "^1",
    "gatsby-transformer-sharp": "^2.2",
    "prop-types": "^15.7",
    "react": "^16.10",
    "react-dom": "^16.10",
    "react-helmet": "^5.2",
    "silverstripe-gatsby-helpers": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^12.11",
    "@types/react": "^16.9",
    "@types/react-dom": "^16.9",
    "node-sass": "^4.12",
    "prettier": "^1.18"
  },
  "keywords": [
    "gatsby",
    "silverstripe"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "ENABLE_GATSBY_REFRESH_ENDPOINT=1 gatsby develop",
